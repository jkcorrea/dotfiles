# Test PR branch
gpr () {
  git fetch upstream pull/$1/head:pr-$1 && git checkout pr-$1;
}

# Kill all processes matching
kl () {
  kill `ps aux | grep "[${1:0:1}]${1:1}" | awk '{print $2}'`
}

