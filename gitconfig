[user]
	email = jcorrea257@gmail.com
	name = Jake Correa
[push]
	default = simple
[core]
	excludesfile = /home/jkcorrea/.gitignore_global
[merge]
  tool = sublimerge
[mergetool "sublimerge"]
  cmd = subl -n --wait \"$REMOTE\" \"$BASE\" \"$LOCAL\" \"$MERGED\" --command \"sublimerge_diff_views\"
  trustExitCode = false
[diff]
  tool = sublimerge
[difftool "sublimerge"]
  cmd = subl -n --wait \"$REMOTE\" \"$LOCAL\" --command \"sublimerge_diff_views {\\\"left_read_only\\\": true, \\\"right_read_only\\\": true}\"
[mergetool]
	keepBackup = false
